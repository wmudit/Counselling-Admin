<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.counsellingadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Blogs"
            android:label="@string/title_activity_blogs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Videos"
            android:label="@string/title_activity_videos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Chat"
            android:label="@string/title_activity_chat"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Counsellor"
            android:label="@string/title_activity_counsellor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CounsellorDisplay"
            android:label="@string/title_activity_counsellor_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChatDisplay"
            android:label="@string/title_activity_chat_display"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>